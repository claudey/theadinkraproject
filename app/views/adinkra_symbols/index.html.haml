- content_for :title do
  Adinkra Symbols | #{t('app.name')}

%p{:id => "notice"}
  = notice

%h1 Adinkra Symbols

%table
  %thead
    %tr
      %th Name
      %th Icon png
      %th{:colspan => "3"}

  %tbody
    - @adinkra_symbols.each do |adinkra_symbol|
      %tr
        %td 
          = adinkra_symbol.name
        %img{"src" => adinkra_symbol.icon_png}
        %td
          = link_to('Show', adinkra_symbol)
        %td
          = link_to('Edit', edit_adinkra_symbol_path(adinkra_symbol))
        %td
          -# = link_to('Destroy', adinkra_symbol, method: :delete, data: { confirm: 'Are you sure?' })

%br

-# = link_to('New Adinkra Symbol', new_adinkra_symbol_path)
